{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "add-module-exports",
    "react-require",
    "transform-decorators-legacy",
    ["module-alias", [
      {"src": "./src/config", "expose": "config"},
      {"src": "./src/app", "expose": "app"},
      {"src": "./src/server", "expose": "server"}
    ]]
  ]
}
